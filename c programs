#include<stdio.h>
 main(){
    int n,m;
    int arr1[n][m],arr2[n][m];
    printf("Entee the order of matrix:\n");
    scanf("%d%d",&n,&m);
    printf("Enter the first matrix here:");
    for(int i =0;i<n;i++){
        for(int j=0;j<m;j++){
            scanf("%d",&arr1[i][j]);}        
    }
    printf("Enter the second matrix here:");
    for(int i =0;i<n;i++){
        for(int j=0;j<m;j++){
            scanf("%d",&arr2[i][j]);}
    }
    printf("The sum of two matrices is:");
    for(int i =0;i<n;i++){
        for(int j=0;j<m;j++){
            printf("%d\t",arr1[i][j]-arr2[i][j]);}
        printf("\n");
    }

